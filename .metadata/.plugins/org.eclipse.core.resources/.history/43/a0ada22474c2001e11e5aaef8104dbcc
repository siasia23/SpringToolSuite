package com.oracle.oBootJpa02.domain;

import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import jakarta.persistence.Table;

@Entity
@Table(name = "member2")
public class Member {

	@Id
	private Long 		id;
	
	@Column
	private String 	name;
	private Long 		sal;
	
}
